<div class="m-dialogBig-form" node-name="dialogForm" style="width: 400px;">
    <ul class="items-box" style="max-height: 300px; overflow-y: auto; margin-bottom: 20px;">
        <% for (var i = 0; i < data.length; i++) { %>
        <li class="item auto" style="line-height: 20px;">
            <input style="float: left; margin: 14px 7px 0 0;" id="<%= data[i].id %>" type="radio" value="<%= data[i].id %>" name="cinemas" data-validate="required=true&nullText=影院"><label for="<%= data[i].id %>" style="float: left; width: 300px; margin-top: 10px; word-wrap: break-word;"><%= data[i].name %></label>
        </li>
        <% } %>
    </ul>
    <div class="remember-config" style="position: absolute; bottom: 70px; left: 45px;">
        <label><input type="checkbox" value="isDefault" name="isDefault" checked="checked" data-validate="nullText=记住影院选项"><span style="margin-left: 10px; line-height: 21px;">记住影院选项</span></label>
    </div>
</div>